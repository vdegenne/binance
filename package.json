			"types": "./lib/batch/BatchesManagerClient.d.ts"
		},
		"./server/binanceExchangeInfo.js": {
			"import": "./lib/BinanceExchangeInfoServer.js",
			"types": "./lib/BinanceExchangeInfoServer.d.ts"
		},
		"./server/batch.js": {
			"import": "./lib/batch/BatchServer.js",
			"types": "./lib/batch/BatchServer.d.ts"
		},
		"./server/batchesManager.js": {
			"import": "./lib/batch/BatchesManagerServer.js",
